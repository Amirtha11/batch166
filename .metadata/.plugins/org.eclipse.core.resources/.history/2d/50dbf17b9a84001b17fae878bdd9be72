package returnTypeTest;

public class ReturnTypeTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//add();
//sub();
		int result=add()+sub();
	}
public static int add(){
	int i=1;
	int j=1;
	int k=i+j;
	System.out.println(k);
	return k;
}

public static int sub(){
	int i=5;
	int j=2;
	int k=i-j;
	System.out.println(k);
	return k;
}
}
