package returnTypeTest;

public class ReturnTypeTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
add();
sub();
	}
public static void add(){
	int i=1;
	int j=1;
	int k=i+j;
	System.out.println(k);
}
public static void sub(){
	int i=5;
	int j=2;
	int k=i-j;
	System.out.println(k);
}
}
